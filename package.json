{
  "name": "saas",
  "version": "1.0.0",
  "description": "Simpsons as a Service",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-babel",
    "dev": "npm run build && nodemon --exec babel-node ./build/index.js",
    "start": "npm run build && node ./build/index.js"
  },
  "keywords": [
    "ACDC2018",
    "Simpsons",
    "API"
  ],
  "author": "damsleth",
  "license": "ISC",
  "dependencies": {
    "express": "^4.16.4"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-preset-env": "1.7.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.10"
  }
}
