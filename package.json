{
  "name": "saas",
  "version": "1.0.0",
  "description": "Simpsons as a Service",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-babel",
    "dev": "npm run build && nodemon --exec babel-node ./build/index.js",
    "start": "npm run build && node ./build/index.js"
  },
  "keywords": [
    "ACDC2018",
    "Simpsons",
    "API"
  ],
  "author": "damsleth",
  "license": "ISC",
  "dependencies": {
    "express": "^4.16.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.10"
  }
}
